        android:id="@+id/settingsFragment"
        android:name="com.techbayportal.itaste.ui.fragments.settingsfragment.SettingsFragment"
        android:label="SettingsFragment" >
        <action
            android:id="@+id/action_settingsFragment_to_changeLanguageFragment"
            app:destination="@id/changeLanguageFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
        <action
            android:id="@+id/action_settingsFragment_to_contactUsFragment"
            app:destination="@id/contactUsFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/changeLanguageFragment"
        android:name="com.techbayportal.itaste.ui.fragments.changelanguagefragment.ChangeLanguageFragment"
        android:label="ChangeLanguageFragment" />
    <fragment
        android:id="@+id/contactUsFragment"
        android:name="com.techbayportal.itaste.ui.fragments.contactusfragment.ContactUsFragment"
        android:label="ContactUsFragment" />
    <fragment
        android:id="@+id/welcomefragment"
        android:name="com.techbayportal.itaste.ui.fragments.welcomefragment.Welcomefragment"
        android:label="Welcomefragment" />
</navigation>