        android:id="@+id/profileFragment"
        android:name="com.techbayportal.itaste.ui.fragments.profilefragment.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/savedPostsFragment"
        android:name="com.techbayportal.itaste.ui.fragments.savedpostsfragment.SavedPostsFragment"
        android:label="SavedPostsFragment" />
    <fragment
        android:id="@+id/searchFragment"
        android:name="com.techbayportal.itaste.ui.fragments.searchfragment.SearchFragment"
        android:label="SearchFragment" />
    <fragment
        android:id="@+id/cartFragment"
        android:name="com.techbayportal.itaste.ui.fragments.cartfragment.CartFragment"
        android:label="CartFragment" />
    <fragment
        android:id="@+id/postFragment"
        android:name="com.techbayportal.itaste.ui.fragments.postfragment.PostFragment"
        android:label="CartFragment" />
