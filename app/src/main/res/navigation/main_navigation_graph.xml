        android:name="com.techbayportal.itaste.ui.fragments.deleteaccountfragment.DeleteAccountFragment"
        android:label="DeleteAccountFragment" >
        <action
            android:id="@+id/action_deleteAccountFragment_to_accountDeletedFragment"
            app:destination="@id/accountDeletedFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/accountDeletedFragment"
        android:name="com.techbayportal.itaste.ui.fragments.accountdeletedfragment.AccountDeletedFragment"
        android:label="AccountDeletedFragment" >
        <action
            android:id="@+id/action_accountDeletedFragment_to_selectAccountTypeFragment"
            app:destination="@id/selectAccountTypeFragment"
            app:enterAnim="@anim/enter_from_right"
            app:exitAnim="@anim/exit_to_left"
            app:popEnterAnim="@anim/enter_from_left"
            app:popExitAnim="@anim/exit_to_right"/>
    </fragment>
    <fragment
        android:id="@+id/selectAccountTypeFragment"
        android:name="com.techbayportal.itaste.ui.fragments.selectaccounttypefragment.SelectAccountTypeFragment"
        android:label="SelectAccountTypeFragment" />
    <fragment
        android:id="@+id/blockedAccountsFragment"
        android:name="com.techbayportal.itaste.ui.fragments.blockedaccountsfragment.BlockedAccountsFragment"
        android:label="BlockedAccountsFragment" />
    <fragment
        android:id="@+id/newMessageFragment"
        android:name="com.techbayportal.itaste.ui.fragments.newmessagefragment.NewMessageFragment"
        android:label="NewMessageFragment" />
    <fragment
        android:id="@+id/changeExistingPasswordFragment"
        android:name="com.techbayportal.itaste.ui.fragments.changeexistingpasswordfragment.ChangeExistingPasswordFragment"
        android:label="ChangeExistingPasswordFragment" />
